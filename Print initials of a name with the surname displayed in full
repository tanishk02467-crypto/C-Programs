#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i = 0;

    printf("Enter a full name: ");
    fgets(str, sizeof(str), stdin);

    str[strcspn(str, "\n")] = '\0';

    if (str[0] != ' ') {
        printf("%c ", str[0]);
    }

    int lastSpace = -1;

    for (i = 1; str[i] != '\0'; i++) {
        if (str[i] == ' ' && str[i + 1] != ' ' && str[i + 1] != '\0') {
            lastSpace = i;
            printf("%c ", str[i + 1]);
        }
    }

    if (lastSpace != -1) {
        printf("%s\n", &str[lastSpace + 1]);
    }

}